[include]
    path = ~/.gitconfig.local

[core]
    excludesfile = ~/.gitignore_global
    pager = delta

[commit]
    template = ~/.stCommitMsg

[http]
    cookiefile = ~/.gitcookies

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true

[merge]
    conflictStyle = zdiff3

